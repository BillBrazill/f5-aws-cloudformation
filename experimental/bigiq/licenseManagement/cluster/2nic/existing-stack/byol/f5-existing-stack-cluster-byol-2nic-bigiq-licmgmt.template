				},
				"restrictedSrcAddressApp": {
					"default": "Source Address(es) for internal Management Access"
				},
				"sshKey": {
					"default": "SSH Key"
				},
				"subnet1Az1": {
					"default": "Subnet1 in AZ1"
				},
				"subnet1Az2": {
					"default": "Subnet1 in AZ2"
				},
				"timezone": {
					"default": "Timezone (Olson)"
				}
			}
